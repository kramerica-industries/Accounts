<?xml version="1.0"?>
<configuration>
  
  <appSettings>
    <add key="Accounts.Value" value="Local Machine"/>
    <add key="Accounts.Environment" value="Development"/>
    <add key="MaxAcceptableOER" value="0.5"/>
  </appSettings>
  
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
